---
import { defaultLocale } from '../i18n/config';

export const prerender = true;

const to = `/${defaultLocale}/tools/`;
return Astro.redirect(to, 302);
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>Redirecting…</title>
  </head>
  <body>
    Redirecting to <a href={to}>{to}</a>…
  </body>
</html>
