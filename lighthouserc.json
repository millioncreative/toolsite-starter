{
  "ci": {
    "collect": {
      "url": ["http://localhost:4173/", "http://localhost:4173/tools"],
      "startServerCommand": "npm run preview -- --host 0.0.0.0",
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": "off",
        "categories:accessibility": "off",
        "categories:best-practices": "off",
        "categories:seo": "off"
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./reports/lh"
    }
  }
}
